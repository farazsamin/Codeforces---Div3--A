/*
Link      :  https://codeforces.com/problemset/problem/1234/A


Approach  :


 
*/
#include<bits/stdc++.h>
using namespace std;


int main(){
   int t,n;
   cin>>t;
   while(t--){
   	  long long sum=0;
   	  cin>>n;
   	  int arr[n];
   	  for(int i=0;i<n;i++){
		  cin>>arr[i];
		  sum+=arr[i];
      }
      cout<<(sum+n-1)/n<<endl;
   }


}
