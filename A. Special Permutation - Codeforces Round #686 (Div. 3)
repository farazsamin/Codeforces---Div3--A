/*
Link      :  https://codeforces.com/problemset/problem/1454/A


Approach  :  Just print i+1 numbers. and print 1 at last.


 
*/
#include<bits/stdc++.h>
using namespace std;


int main(){
	int t,n;
	cin>>t;
	while(t--){
		cin>>n;
		int a=1;
		for(int i=1;i<n;i++) cout<<i+1<<" ";
		cout<<"1"<<endl;
	    
	}
}
